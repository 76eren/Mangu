<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity" android:id="@+id/current">
    <ImageView
            android:layout_width="225dp"
            android:layout_height="326dp" app:srcCompat="@drawable/ic_launcher_background"
            android:id="@+id/curr"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.708"/>
    <ImageView
            android:layout_width="61dp"
            android:layout_height="61dp" app:srcCompat="@drawable/ic_launcher_background"
            android:id="@+id/prev"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.045" app:layout_constraintVertical_bias="0.61"/>
    <ImageView
            android:layout_width="58dp"
            android:layout_height="61dp" app:srcCompat="@drawable/ic_launcher_background"
            android:id="@+id/next"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.963" app:layout_constraintVertical_bias="0.61"/>
    <EditText
            android:layout_width="315dp"
            android:layout_height="63dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/input"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.026" app:layout_constraintHorizontal_bias="0.533"/>
    <Button
            android:text="Search"
            android:layout_width="125dp"
            android:layout_height="66dp" android:id="@+id/search"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.189"/>
    <TextView
            android:layout_width="405dp"
            android:layout_height="51dp" android:id="@+id/name"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.939" android:foregroundGravity="center"
            android:gravity="center_horizontal"/>
</androidx.constraintlayout.widget.ConstraintLayout>